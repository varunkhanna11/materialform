<!-- Material check box -->
<md-checkbox>
<label class="mdl-checkbox mdl-js-checkbox" for="{opts.id}">
<input type="checkbox" id="opts.id" class="mdl-checkbox__input"  isrequired="{opts.isrequired}">
<span class="mdl-checkbox__label">{opts.label}</span>
<span class="mdl-textfield__error">{opts.errormsgreq}</span>
</md-checkbox>
